{
  "schemaVersion": 1,
  "id": "dimensional-inventories",
  "version": "${version}",

  "name": "Dimensional Inventories",
  "description": "A Minecraft Fabric mod for keeping separate inventories across sets of dimensions.",
  "authors": [
    "Thomilist"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/dimensional-inventories",
    "sources": "https://github.com/thomilist/dimensional-inventories/",
    "issues": "https://github.com/Thomilist/dimensional-inventories/issues"
  },

  "license": "MIT",
  "icon": "assets/dimensionalinventories/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.thomilist.dimensionalinventories.DimensionalInventoriesMod"
    ]
  },
  "mixins": [
    "dimensionalinventories.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.1",
    "fabric-api": "*",
    "minecraft": "1.20.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
